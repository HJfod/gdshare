{
  "name": "gdshare",
  "productName": "GDShare",
  "version": "1.0.0",
  "description": "Geometry Dash Backup & Level Sharing Application",
  "main": "app/index.js",
  "scripts": {
    "start": "electron .",
    "make-win32": "electron-packager . gdshare --overwrite --asar --platform=win32 --arch=ia32 --ignore=.gitignore --ignore='data/' --extraResource='EULA.txt' --extraResource='resources/light.gdst' --extraResource='resources/dark.gdst' --extraResource='resources/mid.gdst' --extraResource='resources/amoled.gdst' --icon=resources/share.ico --prune=true --out=release-builds --version-string.CompanyName=HJfod --version-string.FileDescription=CE --version-string.ProductName=\"GD Share\""
  },
  "author": "HJfod",
  "license": "ISC",
  "devDependencies": {
    "electron": "^9.0.4",
    "electron-packager": "^15.0.0",
    "electron-winstaller": "^4.0.1"
  },
  "dependencies": {
    "auto-launch": "^5.0.5",
    "ncp": "^2.0.0",
    "pako": "^1.0.11"
  },
  "production": false
}
